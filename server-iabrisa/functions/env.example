# Configurações do servidor backend
# Copie este arquivo para .env e preencha com suas credenciais

# Caminho para o arquivo de credenciais do Google Cloud
GOOGLE_APPLICATION_CREDENTIALS=./src/config/firebase/credential/account.json

# ID do projeto Firebase
FIREBASE_PROJECT_ID=seu-projeto

# Configurações do ambiente
NODE_ENV=development
PORT=5001

# Outras configurações
API_VERSION=v1
CORS_ORIGIN=http://localhost:3000

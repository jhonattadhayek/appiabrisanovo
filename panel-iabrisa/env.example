# Configurações do Firebase para o painel administrativo
# Copie este arquivo para .env e preencha com suas credenciais

REACT_APP_FIREBASE_API_KEY=sua-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=seu-projeto.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=seu-projeto
REACT_APP_FIREBASE_STORAGE_BUCKET=seu-projeto.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=seu-messaging-sender-id
REACT_APP_FIREBASE_APP_ID=seu-app-id

# Outras configurações do painel
REACT_APP_API_URL=https://sua-api-url.com
REACT_APP_ENVIRONMENT=development
